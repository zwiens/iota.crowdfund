{
  "exclude": [
    "node_modules"
  ],
  "include": [
    "app/**/*",
    "app/*"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "lib": ["es2015", "dom"],
    "outDir": "dist"
  }
}